<!doctype html>
<html lang="fr-FR">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <title>{% block title %}Sortir.com{% endblock %}</title>

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link href="https://fonts.googleapis.com/css2?family=Crimson+Text:ital@1&family=Roboto:wght@100&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="">
    <link rel="stylesheet" href="">

    <link rel="icon" href="">

    {% block extrastyles %}{% endblock %}

    {% block javasripts %}
        {% block importmap %} {{ importmap('app') }} {% endblock %}
    {% endblock %}

</head>
<body>
<div class="container">
    <header>
       {#  <h1><a href="{{ path("app_main") }}"><img src="" alt="Series!"></a></h1>
        {% include("inc/nav.html.twig") %}#}
        <h2>Logo Sortir.com</h2>
    </header>

    {# read and display all flash messages #}
    {% for label, messages in app.flashes %}
        {% for message in messages %}
            <div class="flash flash-{{ label }}">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}



    <main>
        {% block body %}{% endblock %}
    </main>
    <footer>
        {#  {% include('inc/nav.html.twig') %}#}
        <div>&copy; {{ "now" | date("Y") }} Sortir.com!</div>
    </footer>
</div>
</body>
</html>
